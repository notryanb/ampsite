<%= form_for :user, url: signup_path do |f| %>
  <%= f.label :user %><br/>
  <%= f.text_field :username %><br/><br/>

  <%= f.label :email %><br/>
  <%= f.text_field :email %><br/><br/>

  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>
  
  <%= f.submit "Sign up!" %>

<% end %>