<% @users.each do |user| %>
  <li> <%= user.username %> / <%= user.email %> / 
<% end %>

<br/><br/>

<% if current_user %>
  <p> You are logged in as <%= current_user.username %> </p>
  <%= link_to 'Logout', logout_path %>
<% else %>
  <p> You are not logged in </p>
  <%= render partial: 'login' %>
<% end %>