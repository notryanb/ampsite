<div class="row">
    <div class="large-12 columns">

      <div class="row">
        <div class="large-12 columns">
          <%= render partial: 'shared/tubeadmin', locals: {tube: @diode} %>
        </div>
      </div>

      <table class="posts large-12 columns">
        <tr>
          <th scope="column" class="topic-header">Identifier</th>
          <th scope="column" class="topic-header">Class</th>
          <th scope="column" class="topic-header">PIV</th>
          <th scope="column" class="topic-header">Voltage Drop</th>
          <th scope="column" class="topic-header">RMS Anode-Anode</th>
          <th scope="column" class="topic-header">Ih</th>
          <th scope="column" class="topic-header">Socket Type</th>
        </tr>

        <tr>
          <td><%= @diode.identifier %></td>
          <td><%= @diode.rectifier_class %></td>
          <td><%= @diode.max_peak_inverse_voltage %> vDC</td>
          <td><%= @diode.voltage_drop %> vDC</td>
          <td><%= @diode.rms_anode_to_anode %> vAC</td>
          <td><%= @diode.filament_current %> mA</td>
          <td><%= @diode.tubesocket? %></td>
        </tr>
      </table>

    </div>
</div>